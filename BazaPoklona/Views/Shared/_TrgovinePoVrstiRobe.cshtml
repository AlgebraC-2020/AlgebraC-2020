@model IEnumerable<BazaPoklona.Models.Trgovina>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NazivTrgovina)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VrstaRobeNavigation)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NazivTrgovina)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VrstaRobeNavigation.Naziv)
            </td>
        </tr>
}
    </tbody>
</table>
